<h1>Refreshes#index</h1>
<p>Find me in app/views/refreshes/index.html.erb</p>
<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>
<% @refrehes.each do |n| %>
	<li> <%= n.device_name + " | " + n.device_type %> 
	<%= link_to "edit", edit_refresh_path(n) %>
	<%= link_to "show",refresh_path(n) %>
	</li>
<% end %>
